-		board_nand_data.gpmc_t		= &am335x_nand_timings;
+		board_nand_data.ecc_opt  = OMAP_ECC_BCH8_CODE_HW;
+		board_nand_data.xfer_type = NAND_OMAP_PREFETCH_POLLED;
 	}
 
 	gpmc_nand_init(&board_nand_data);
