 static struct omap_nand_platform_data board_nand_data = {
 	.gpmc_t		= &nand_timings,
 };
@@ -171,9 +148,8 @@ __init board_nand_init(struct mtd_partition *nand_parts,
 	board_nand_data.gpmc_irq = OMAP_GPMC_IRQ_BASE + cs;
 
 	if (cpu_is_am335x()) {
