-
-	.cs_on = 0,
-	.cs_rd_off = 44,
-	.cs_wr_off = 44,
-
-	.adv_on = 6,
-	.adv_rd_off = 34,
-	.adv_wr_off = 44,
-
-	.we_off = 40,
-	.oe_off = 54,
-
-	.access = 64,
-	.rd_cycle = 82,
-	.wr_cycle = 82,
-
