 	.clkdm_name	= "l4ls_clkdm",
 	.mpu_irqs	= am33xx_ehrpwm2_irqs,
@@ -857,7 +857,7 @@ static struct omap_hwmod_irq_info am33xx_ecap0_irqs[] = {
 };
 
 static struct omap_hwmod am33xx_ecap0_hwmod = {
-	.name		= "ecap.0",
+	.name		= "ecap0",
