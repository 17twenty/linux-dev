 };
 
 static struct omap_hwmod am33xx_ehrpwm0_hwmod = {
-	.name		= "ehrpwm.0",
+	.name		= "ehrpwm0",
 	.class		= &am33xx_epwmss_hwmod_class,
 	.clkdm_name	= "l4ls_clkdm",
