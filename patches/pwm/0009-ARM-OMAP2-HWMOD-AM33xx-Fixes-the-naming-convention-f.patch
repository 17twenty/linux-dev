-	.name		= "ecap.1",
+	.name		= "ecap1",
 	.class		= &am33xx_epwmss_hwmod_class,
 	.clkdm_name	= "l4ls_clkdm",
 	.mpu_irqs	= am33xx_ecap1_irqs,
@@ -897,7 +897,7 @@ static struct omap_hwmod_irq_info am33xx_ecap2_irqs[] = {
 };
 
 static struct omap_hwmod am33xx_ecap2_hwmod = {
-	.name		= "ecap.2",
+	.name		= "ecap2",
