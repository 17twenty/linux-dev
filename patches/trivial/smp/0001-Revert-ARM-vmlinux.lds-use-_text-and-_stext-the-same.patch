From 5629bce2f73a2fc03bdb06dfacdb208410b24c22 Mon Sep 17 00:00:00 2001
From: Robert Nelson <robertcnelson@gmail.com>
Date: Tue, 26 Jul 2011 15:28:41 -0500
Subject: [PATCH 1/5] Revert "ARM: vmlinux.lds: use _text and _stext the same way as x86"

This reverts commit e2f81844efa2d44d326bef48e1c9e48926162bc6.
---
 arch/arm/kernel/vmlinux.lds.S |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/kernel/vmlinux.lds.S b/arch/arm/kernel/vmlinux.lds.S
index bf977f8..18574b7 100644
--- a/arch/arm/kernel/vmlinux.lds.S
+++ b/arch/arm/kernel/vmlinux.lds.S
@@ -66,11 +66,11 @@ SECTIONS
 	. = PAGE_OFFSET + TEXT_OFFSET;
 #endif
 	.head.text : {
-		_text = .;
+		_stext = .;
 		HEAD_TEXT
 	}
 	.text : {			/* Real text segment		*/
-		_stext = .;		/* Text and read-only data	*/
+		_text = .;		/* Text and read-only data	*/
 			__exception_text_start = .;
 			*(.exception.text)
 			__exception_text_end = .;
-- 
1.7.4.1

